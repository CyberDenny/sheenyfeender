{
	"schemaVersion": 1,
	"id": "shinyfinder",
	"version": "1.0.3",
	"name": "Shiny Finder",
	"description": "A mod that finds shiny Pokemon in the surrounding area and alerts you when one is discovered!",
	"authors": [
		"Ned"
	],
	"contact": {
		"homepage": "https://nednet.pages.dev/tutorials/shiny-finder",
		"sources": "https://nednet.pages.dev/tutorials/shiny-finder"
	},
	"license": "MIT",
	"icon": "assets/shinyfinder/icon.png",
